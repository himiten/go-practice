{
  "desc": "Auto generated by fswatch [web-martini]",
  "triggers": [
    {
      "name": "web-martini",
      "pattens": [
        "**/*.go",
        "**/*.c"
      ],
      "env": {
        "DEBUG": "1"
      },
      "cmd": "go run -v server.go",
      "shell": false,
      "delay": "100ms",
      "stop_timeout": "500ms",
      "signal": "KILL",
      "kill_signal": ""
    }
  ],
  "watch_paths": [
    "."
  ],
  "watch_depth": 1
}